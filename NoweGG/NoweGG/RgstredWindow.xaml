<Window x:Class="NoweGG.RgstredWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NoweGG"
        mc:Ignorable="d"
        Title="RgstredWindow" Height="150" Width="200">
    <Grid>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" TextWrapping="Wrap" Text="Registration went fine" VerticalAlignment="Top" Margin="25,25,0,0" Height="25" Width="150" TextAlignment="Center"/>
        <Button x:Name="button" Content="Go Back" HorizontalAlignment="Left" VerticalAlignment="Top" Width="100" Margin="50,75,0,0" Click="button_Click"/>

    </Grid>
</Window>
