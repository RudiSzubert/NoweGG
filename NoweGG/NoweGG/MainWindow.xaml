<Window x:Class="NoweGG.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NoweGG"
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="250" WindowStartupLocation="CenterScreen">
    <Grid>
        <Button x:Name="buttonUser" Content="User" HorizontalAlignment="Left" VerticalAlignment="Top" Width="150" RenderTransformOrigin="1.475,4.379" Margin="50,40,0,0" Height="50" FontSize="20" Click="buttonUser_Click"/>
        <Button x:Name="buttonGGServer" Content="GGServer" HorizontalAlignment="Left" VerticalAlignment="Top" Width="150" RenderTransformOrigin="1.475,4.379" Margin="50,110,0,0" Height="50" FontSize="20" Click="buttonGGServer_Click"/>
        <Button x:Name="buttonExit" Content="Exit" HorizontalAlignment="Left" VerticalAlignment="Top" Width="150" RenderTransformOrigin="1.475,4.379" Margin="50,180,0,0" Height="50" FontSize="20" Click="buttonExit_Click"/>

    </Grid>
</Window>
